{
  "name": "similarities",
  "version": "1.0.0",
  "author": "maxgfr",
  "license": "MIT",
  "description": "A typescript library to calculate similarities between two arrays",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/similarities.git"
  },
  "bugs": {
    "url": "https://github.com/maxgfr/similarities/issues"
  },
  "homepage": "https://github.com/maxgfr/similarities#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "similarities",
    "pearson"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^10.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^9.0.0",
    "@semantic-release/npm": "^10.0.0",
    "@semantic-release/release-notes-generator": "^11.0.0",
    "@swc-node/register": "1.10.9",
    "@swc/cli": "0.6.0",
    "@swc/core": "1.10.9",
    "@swc/jest": "0.2.37",
    "@types/jest": "29.5.14",
    "@types/node": "18.19.73",
    "@typescript-eslint/eslint-plugin": "5.62.0",
    "@typescript-eslint/parser": "5.62.0",
    "eslint": "8.57.1",
    "eslint-config-prettier": "8.10.0",
    "eslint-plugin-jest": "27.9.0",
    "jest": "29.7.0",
    "nodemon": "3.1.9",
    "prettier": "3.4.2",
    "rimraf": "5.0.10",
    "semantic-release": "21.1.2",
    "typescript": "5.7.3"
  }
}
