{
  "name": "similarities",
  "version": "1.0.0",
  "author": "maxgfr",
  "license": "MIT",
  "description": "A typescript library to calculate similarities between two arrays",
  "main": "./build/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/maxgfr/similarities.git"
  },
  "bugs": {
    "url": "https://github.com/maxgfr/similarities/issues"
  },
  "homepage": "https://github.com/maxgfr/similarities#readme",
  "files": [
    "build"
  ],
  "keywords": [
    "similarities",
    "pearson"
  ],
  "scripts": {
    "start": "node build/index.js",
    "dev": "nodemon",
    "develop": "node -r @swc-node/register ./src/index.ts",
    "test": "jest --passWithNoTests",
    "test:watch": "jest --watch",
    "clean": "rimraf build",
    "build": "tsc -p tsconfig.build.json",
    "build:watch": "tsc -w -p tsconfig.build.json",
    "build:swc": "swc ./src -d build",
    "build:swc:watch": "swc ./src -d build -w",
    "lint": "eslint ./src --ext .ts",
    "prettier": "prettier --write './src/**/*.{ts,js,json}'",
    "release": "semantic-release"
  },
  "dependencies": {},
  "devDependencies": {
    "@semantic-release/changelog": "^6.0.2",
    "@semantic-release/commit-analyzer": "^13.0.0",
    "@semantic-release/git": "^10.0.1",
    "@semantic-release/github": "^12.0.0",
    "@semantic-release/npm": "^13.0.0",
    "@semantic-release/release-notes-generator": "^14.0.0",
    "@swc-node/register": "1.11.1",
    "@swc/cli": "0.7.8",
    "@swc/core": "1.13.20",
    "@swc/jest": "0.2.39",
    "@types/jest": "30.0.0",
    "@types/node": "22.18.11",
    "@typescript-eslint/eslint-plugin": "8.46.1",
    "@typescript-eslint/parser": "8.46.1",
    "eslint": "9.38.0",
    "eslint-config-prettier": "10.1.8",
    "eslint-plugin-jest": "29.0.1",
    "jest": "30.2.0",
    "nodemon": "3.1.10",
    "prettier": "3.6.2",
    "rimraf": "6.0.1",
    "semantic-release": "25.0.0",
    "typescript": "5.9.3"
  }
}
